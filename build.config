<project>

  <!-- If automated.build hasn't been defined, default it to false -->
  <ifnot test="${property::exists('automated.build')}"
    <property name="automated.build" value="false" />
  </ifnot>

  <!-- Set defaults for a manual build -->
  <ifnot test="${automated.build}">
  </ifnot>
  
  <!-- Set defaults for an automated build -->
  <ifnot test="${automated.build">
  </ifnot>
</project>