    <SendAsAttachments>false</SendAsAttachments>
  </NewsletterConfiguration>
  <OverrideBordersScope>Namespace</OverrideBordersScope>
  <RequireCaptchaOnEdit>Always</RequireCaptchaOnEdit>
  <SendNamespaceCreationMailToCC />
