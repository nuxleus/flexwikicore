          <Parameter Name="ConnectionString" Value="Data Source=.\SQL2005;Initial Catalog=FlexWikiSqlStore;Integrated Security=SSPI;" />
        </Parameters>
      </Provider>
      <Provider Id="e7aecc18-9b22-465f-bd89-7efbd8dd425a" Type="FlexWiki.FileSystemNamespaceProvider" AssemblyName="FlexWiki, Version=2.0.0.0, Culture=neutral, PublicKeyToken=null">
